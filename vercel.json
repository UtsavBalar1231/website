{
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/(css|js|icons)/(.*)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(wasm|js)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/(.*)\\.(ico|jpg|jpeg|png|gif|svg|webp)",
      "headers": [
        {
          "key": "cache-control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "x-content-type-options",
          "value": "nosniff"
        },
        {
          "key": "x-frame-options",
          "value": "DENY"
        },
        {
          "key": "x-xss-protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!css|js|icons|manifest\\.json|sw\\.js|robots\\.txt|favicon\\.).*)",
      "destination": "/$1/index.html"
    }
  ]
}